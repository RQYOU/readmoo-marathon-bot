{
  "name": "readmoo-marathon-bot",
  "version": "1.0.0",
  "description": "Readmoo bot for marathons - for education purpose only",
  "main": "app.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "docker:build": "docker build -t vansteki/readmoo-bot:latest .",
    "docker:container": "docker run -d --rm vansteki/readmoo-bot:latest -b ",
    "docker:containers": "cat books.txt | xargs -I book docker run -d --rm vansteki/readmoo-bot:latest -b book",
    "start": "node app"
  },
  "repository": {
    "type": "git",
    "url": "http://codebase.local/dev/readmoo-bot"
  },
  "keywords": [
    "Puppeteer",
    "Chrome",
    "Headless",
    "Scrapping"
  ],
  "author": "vance",
  "license": "MIT",
  "dependencies": {
    "command-line-args": "^5.1.1",
    "puppeteer-core": "^5.2.1",
    "yaml": "^1.10.0"
  },
  "devDependencies": {
    "puppeteer": "^5.2.1"
  }
}
